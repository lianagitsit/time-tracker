<form>
    <div class="form-group">
        <label for="summary">Event name: </label>
        <input type="text" class="form-control" id="summary" placeholder="Whats the event name?">
    </div>
    <div class="form-group">
        <label for="location">Location: </label>
        <input type="text" class="form-control" id="location" placeholder="Where is the event?">
    </div>
    <div class="form-group">
        <label for="start">Start time: </label>
        <input type="datetime-local" class="form-control" id="start" placeholder="What time does it start? (ex: HH:MM DD:MM:YYYY)">
    </div>
    <div class="form-group">
        <label for="end">End time: </label>
        <input type="datetime-local" class="form-control" id="end" placeholder="What time does it end? (ex: HH:MM DD:MM:YYYY)">
    </div>
    <button type="submit" class="btn btn-primary" onclick="stuff()">Submit</button>
</form>

<script>
    function stuff() {
        event.preventDefault();
        var summary = $("#summary").val().trim();
        var location = $("#location").val().trim();
        var inputStart = $("#start").val().trim();
        var inputEnd = $("#end").val().trim();
        console.log(inputStart);
        console.log(inputEnd);

        var start = moment(inputStart, "YYYY-MM-DD HH:mm").format();
        var end = moment(inputEnd, "YYYY-MM-DD HH:mm").format();

        var resource = {
            "summary": summary,
            "location": location
            "start": {
                "dateTime": start
            },
            "end": {
                "dateTime": end
            }
        };
    };

    console.log(moment("05/24/2018 10:00pm", "MM-DD-YYYY hA:mm").format());
</script>